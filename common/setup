#bootstrap
ln -s /usr/local/lib/node_modules/bootstrap/LICENSE licenses/bootstrap.LICENSE
ln -s /usr/local/lib/node_modules/bootstrap/dist/js/bootstrap.js js/
ln -s /usr/local/lib/node_modules/bootstrap/dist/js/bootstrap.js.map js/
ln -s /usr/local/lib/node_modules/bootstrap/dist/js/bootstrap.min.js js/
ln -s /usr/local/lib/node_modules/bootstrap/dist/js/bootstrap.min.js.map js/
ln -s /usr/local/lib/node_modules/bootstrap/dist/css/bootstrap.css css/
ln -s /usr/local/lib/node_modules/bootstrap/dist/css/bootstrap.css.map css/
ln -s /usr/local/lib/node_modules/bootstrap/dist/css/bootstrap.min.css css/
ln -s /usr/local/lib/node_modules/bootstrap/dist/css/bootstrap.min.css.map css/
ln -s /usr/local/lib/node_modules/jquery/dist/jquery.js js/
ln -s /usr/local/lib/node_modules/jquery/dist/jquery.min.js js/


#popper.js
#Missing license
ln -s /usr/local/lib/node_modules/popper.js/dist/umd/popper.js js/
